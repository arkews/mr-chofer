{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@base/*": [
        "*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@navigation/*": [
        "navigation/*"
      ],
      "@screens/*": [
        "screens/*"
      ],
      "@shared/*": [
        "shared/*"
      ]
    }
  }
}
